{
  "name": "gradle-dependency-diff",
  "version": "1.0.0",
  "private": true,
  "description": "Provides a comparison report of transitive dependencies for new maven dependencies",
  "scripts": {
    "build": "tsc && ncc build",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "dev:eg1": "tsc && node lib/dev.js test-assets/eg1-before.txt test-assets/eg1-after.txt",
    "dev:eg2": "tsc && node lib/dev.js test-assets/eg2-before.txt test-assets/eg2-after.txt",
    "dev:eg3": "tsc && node lib/dev.js test-assets/eg3-before.txt test-assets/eg3-after.txt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thementalgoose/gradle-dependency-diff.git"
  },
  "keywords": [],
  "author": "Jordan Fisher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thementalgoose/gradle-dependency-diff/issues"
  },
  "homepage": "https://github.com/thementalgoose/gradle-dependency-diff",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "typescript": "^5.9.2"
  }
}
